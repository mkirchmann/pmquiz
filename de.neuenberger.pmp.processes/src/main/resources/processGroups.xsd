<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="PMP_Processes" type="cplxProcessGroups">
		<xs:annotation>
			<xs:documentation>Comment describing your root element</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="cplxProcessGroups">
		<xs:sequence>
			<xs:element name="KnowledgeArea" type="cplxKnowledgeArea" maxOccurs="unbounded"/>
			<xs:element name="ProcessGroup" type="cplxProcessGroup" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="cplxProcessGroup">
		<xs:complexContent>
			<xs:extension base="cplxNamed">
				<xs:sequence>
					<xs:element name="Process" maxOccurs="unbounded">
						<xs:complexType>
							<xs:complexContent>
								<xs:extension base="cplxProcess">
									<xs:attribute name="knowledgeAreaId" type="xs:string"/>
								</xs:extension>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="cplxProcess">
		<xs:complexContent>
			<xs:extension base="cplxNamed"/>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="cplxNamed">
		<xs:attribute name="name" type="xs:string"/>
	</xs:complexType>
	<xs:complexType name="cplxKnowledgeArea">
		<xs:complexContent>
			<xs:extension base="cplxNamed">
				<xs:sequence>
					<xs:element name="Process" type="cplxProcess"
						minOccurs="0" maxOccurs="unbounded" />
					<xs:element name="Addition" type="cplxAddition" maxOccurs="1" minOccurs="0"></xs:element>
				</xs:sequence>
				<xs:attribute name="id" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="cplxAddition">

		<xs:sequence>
			<xs:element name="Group" type="cplxGroup" maxOccurs="unbounded"></xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="cplxGroup">
		<xs:complexContent>
			<xs:extension base="cplxNamed">
				<xs:sequence>
					<xs:element name="Definition" type="cplxDefinition" maxOccurs="unbounded" minOccurs="4"></xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="cplxDefinition">
		<xs:complexContent>
			<xs:extension base="cplxNamed">
				<xs:attribute name="description" type="xs:string"></xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
